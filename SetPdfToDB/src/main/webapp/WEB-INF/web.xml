<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SetPdfToDB</servlet-name>
  	<display-name>SetPdfToDB</display-name>
  	<description></description>
  	<servlet-class>org.belov.oleg.SetPdfToDB</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetPdf</servlet-name>
  	<display-name>GetPdf</display-name>
  	<description></description>
  	<servlet-class>org.belov.oleg.GetPdf</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SetPdfToDB</servlet-name>
  	<url-pattern>/set</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetPdf</servlet-name>
  	<url-pattern>/GetPdf</url-pattern>
  </servlet-mapping>
</web-app>
